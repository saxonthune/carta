{
  "name": "@carta/web-client",
  "private": true,
  "version": "0.1.0-proto4",
  "type": "module",
  "scripts": {
    "dev": "VITE_STORAGE_BACKENDS=both VITE_COLLABORATION=enabled vite",
    "dev:local": "VITE_STORAGE_BACKENDS=local VITE_COLLABORATION=disabled vite",
    "dev:client": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "vitest run && playwright test"
  },
  "dependencies": {
    "@dagrejs/dagre": "^2.0.3",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@xyflow/react": "^12.10.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "uuid": "^13.0.0",
    "y-indexeddb": "^9.0.12",
    "yjs": "^13.6.29",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^3.0.0",
    "@vitest/ui": "^3.0.0",
    "jsdom": "^26.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^3.0.0",
    "y-websocket": "^3.0.0"
  }
}
